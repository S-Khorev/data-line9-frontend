  </div>

  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.customerId.name
      class=(if (v-get validationObject "customerId.name" "isInvalid") "error")
      label=(t "forms.data-line-data-line9-service-e.customerId.name-caption")
      data-test-data-line-data-line9-service-e----------------=true
    }}
    {{flexberry-validationmessage error=(v-get validationObject "customerId.name" "messages")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.contragentId.name
      class=(if (v-get validationObject "contragentId.name" "isInvalid") "error")
      label=(t "forms.data-line-data-line9-service-e.contragentId.name-caption")
      data-test-data-line-data-line9-service-e------------------=true
    }}
    {{flexberry-validationmessage error=(v-get validationObject "contragentId.name" "messages")}}
  </div>
</form>